<form>
  <field name="name"/>
  <field name="username"/>
  <field name="email"/>
  <field name="password" cols="3"/>
  <field name="active" cols="3"/>
  <field name="is_staff"/>
  <field name="is_superuser"/>
  <div class="col-3">
    <button type="tag" class="btn btn-outline-secondary" name="auth.ChangePassword" target="new">{{ gettext('Change password') }}</button>
  </div>
  <field name="user_action"/>
  <field name="date_joined"/>
  <field name="signature"/>
  <field name="user_company"/>
  <field name="groups"/>
</form>
