    <script src="/static/admin/assets/js/angular/i18n/angular-locale_{{ LANGUAGE_CODE }}.js"></script>
    <script src="/static/admin/assets/js/select2/select2_locale_{{ LANGUAGE_CODE }}.min.js"></script>
  {% endif %}


<div id="loading-msg" class="animated fadeIn">
  <span>{{ gettext('Loading...') }}</span>
</div>
<div id="overlay" class="animated fadeIn text-center">
  <i class="fa-4x fas fa-circle-notch fa-spin"></i>
  <h3 class="margin-top-16">
    {{ gettext('Loading...') }}
  </h3>
</div>

{% block page_loaded %}{% endblock %}
{% block additional_assets %}{% endblock %}

</body>
</html>
